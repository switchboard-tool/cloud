{
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["member"]
    },
    {
      "route": "/api/environments",
      "serve": "/environments.json",
      "allowedRoles": ["member"]
    },
    {
      "route": "/sign-in",
      "serve": "/.auth/login/aad"
    },
    {
      "route": "/welcome",
      "serve": "/welcome.html"
    },
    {
      "route": "/sign-out",
      "serve": "/.auth/logout"
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "Unauthenticated",
      "statusCode": "302",
      "serve": "/welcome"
    }
  ]
}